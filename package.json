{
  "name": "jsbin-api",
  "version": "1.0.2",
  "private": true,
  "engines": {
    "node": "8.x.x"
  },
  "description": "jsbin's API (refactored)",
  "main": "lib/server.js",
  "scripts": {
    "pretest": "rm -rf jsbin-test.sqlite",
    "test": "NODE_ENV=test jest --setupTestFrameworkScriptFile=@remy/envy tests/**/*.test.js",
    "tunnel": "lt --port $(($(envy PORT)+1)) -s jsbin",
    "port-forward": "ssh -g -L $(($(envy PORT)+1)):localhost:$(envy PORT) localhost -N",
    "dev": "npm run dev-server & npm run port-forward & npm run tunnel",
    "dev-server": "GIT_REV=$(git rev-parse HEAD) NODE_ENV=development nodemon -w lib .",
    "start": "node ."
  },
  "keywords": [
    "jsbin"
  ],
  "author": "Remy Sharp <remy@leftlogic.com>",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^16.3.2",
    "@remy/envy": "^3.1.1",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "jsbin-id": "^1.0.0",
    "jsonwebtoken": "^7.4.3",
    "lodash.pick": "^4.4.0",
    "lru-cache": "^4.1.3",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.11",
    "request": "^2.87.0",
    "sequelize": "^5.15.1",
    "serve-favicon": "^2.5.0",
    "server-timings": "^1.0.4",
    "stripe": "^4.25.0",
    "undefsafe": "^2.0.2",
    "universal-analytics": "^0.4.16",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@remy/eslint": "^1.3.0",
    "ajv": "^5.5.2",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^21.2.1",
    "localtunnel": "^1.9.0",
    "sqlite3": "^3.1.13",
    "supertest": "^3.1.0"
  }
}
